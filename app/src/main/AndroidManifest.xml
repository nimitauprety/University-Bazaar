<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.cse3310defaultproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CSE3310DefaultProject"
        android:usesCleartextTraffic="true"
        tools:targetApi="n">
        <activity android:name=".Clubs"></activity>
        <activity android:name=".Trade" />
        <activity android:name=".Sell" />
        <activity android:name=".Buy" />
        <activity android:name=".Marketplace" />
        <activity android:name=".CreateAd" />
        <activity android:name=".Checkout" />
        <activity android:name=".Chat" />
        <activity
            android:name=".Popup"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".Advertisement" />
        <activity android:name=".HomeScreen" />
        <activity android:name=".PopupActivity" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".ChequeActivity" />
        <activity android:name=".CreditCardActivity" />

        <activity android:name=".CreateClubActivity" />
        <activity android:name=".MyClubsActivity" />
        <activity android:name=".JoinClubActivity" />

        <activity
            android:name=".Registration"
            android:label="@string/title_activity_registration"
            android:theme="@style/Theme.CSE3310DefaultProject.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>